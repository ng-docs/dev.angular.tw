                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      \n</div>\n\n\n<section class=\"see-also\">\n  <h2 id=\"see-also\">参见<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/router/withDisabledInitialNavigation#see-also\"><i class=\"material-icons\">link</i></a></h2>\n  <ul>\n    <li><a href=\"api/router/provideRouter\"><code>provideRouter</code></a></li>\n  </ul>\n</section>\n\n<section class=\"description\">\n  <h2 id=\"description\">说明<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/router/withDisabledInitialNavigation#description\"><i class=\"material-icons\">link</i></a></h2>\n  <p data-ng_translator_product=\"100\" data-ng_translator_ref_id=\"8olpmk736k41uanmb2kcxci4r\">如果由于某些复杂的初始化逻辑而有理由对路由器何时开始其初始导航有更多控制权，可以使用。</p>\n  <p data-ng_translator_product=\"100\" data-ng_translator_ref_id=\"2qj93ai4hi9rdr1wvcf1ae2t7\">更多信息请参见\n<a href=\"api/router/withDisabledInitialNavigation#usage-notes\">用法说明...</a></p>\n</section>\n\n\n<section class=\"usage-notes\">\n  <h2 id=\"usage-notes\">使用说明<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/router/withDisabledInitialNavigation#usage-notes\"><i class=\"material-icons\">link</i></a></h2>\n  <p data-ng_translator_product=\"100\" data-ng_translator_ref_id=\"6roh3k83qb53xt45sztwbzt2z\">如何禁用初始导航的基本示例：</p>\n<code-example>\nconst appRoutes: <a href=\"api/router/Routes\" class=\"code-anchor\">Routes</a> = [];\n<a href=\"api/platform-browser/bootstrapApplication\" class=\"code-anchor\">bootstrapApplication</a>(AppComponent,\n  {\n    providers: [\n      <a href=\"api/router/provideRouter\" class=\"code-anchor\">provideRouter</a>(appRoutes, <a href=\"api/router/withDisabledInitialNavigation\" class=\"code-anchor\">withDisabledInitialNavigation</a>())\n    ]\n  }\n);\n</code-example>\n\n</section></div>\n</article>\n\n<!-- links to this doc:\n - api/router\n - api/router/DisabledInitialNavigationFeature\n - api/router/InitialNavigationFeature\n-->\n<!-- links from this doc:\n - /api\n - api/platform-browser/bootstrapApplication\n - api/router\n - api/router/DisabledInitialNavigationFeature\n - api/router/Routes\n - api/router/provideRouter\n - api/router/withDisabledInitialNavigation#description\n - api/router/withDisabledInitialNavigation#see-also\n - api/router/withDisabledInitialNavigation#usage-notes\n - api/router/withDisabledInitialNavigation#withdisabledinitialnavigation\n - https://github.com/ng-docs/angular-cn/tree/17.3.0-next.1/packages/router/src/provide_router.ts#L399-L441\n-->"}