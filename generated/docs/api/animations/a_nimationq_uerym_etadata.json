\ninterface <a href=\"api/animations/AnimationQueryMetadata\" class=\"code-anchor\">AnimationQueryMetadata</a> extends <a href=\"api/animations/AnimationMetadata\" class=\"code-anchor\">AnimationMetadata</a> {\n  <a class=\"code-anchor\" href=\"api/animations/AnimationQueryMetadata#selector\"><span class=\"member-name\">selector</span>: string</a>\n  <a class=\"code-anchor\" href=\"api/animations/AnimationQueryMetadata#animation\"><span class=\"member-name\">animation</span>: AnimationMetadata | AnimationMetadata[]</a>\n  <a class=\"code-anchor\" href=\"api/animations/AnimationQueryMetadata#options\"><span class=\"member-name\">options</span>: AnimationQueryOptions | null</a>\n\n// 繼承自 <a class=\"code-anchor\" href=\"api/animations/AnimationMetadata\">animations/AnimationMetadata</a>\n  <a class=\"code-anchor\" href=\"api/animations/AnimationMetadata#type\"><span class=\"member-name\">type</span>: AnimationMetadataType</a>\n}\n</code-example>\n\n\n</section>\n\n  \n  \n<section class=\"instance-properties\">\n  <a id=\"properties\"></a>\n  <h2 id=\"instance-properties\">屬性<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/animations/AnimationQueryMetadata#instance-properties\"><i class=\"material-icons\">link</i></a></h2>\n  <table class=\"is-full-width list-table property-table\">\n    <thead>\n      <tr>\n        <th>屬性</th>\n        <th>說明</th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr class=\"instance-property\">\n        <td>\n          <a id=\"selector\"></a>\n          <code class=\"\"><span class=\"member-name\">selector</span>: string</code>\n        </td>\n        <td>\n          <p data-ng_translator_product=\"100\" data-ng_translator_ref_id=\"1pubybd47grc0paalp2nhzvpj\"> 該查詢的 CSS 選擇器。</p>\n          \n        </td>\n      </tr>\n      <tr class=\"instance-property\">\n        <td>\n          <a id=\"animation\"></a>\n          <code class=\"\"><span class=\"member-name\"><a href=\"api/animations/animation\" class=\"code-anchor\">animation</a></span>: <a href=\"api/animations/AnimationMetadata\" class=\"code-anchor\">AnimationMetadata</a> | <a href=\"api/animations/AnimationMetadata\" class=\"code-anchor\">AnimationMetadata</a>[]</code>\n        </td>\n        <td>\n          <p data-ng_translator_product=\"100\" data-ng_translator_ref_id=\"319bycz23llf0ki36wnkr7de9\"> 一個或多個動畫步驟物件。</p>\n          \n        </td>\n      </tr>\n      <tr class=\"instance-property\">\n        <td>\n          <a id=\"options\"></a>\n          <code class=\"\"><span class=\"member-name\">options</span>: <a href=\"api/animations/AnimationQueryOptions\" class=\"code-anchor\">AnimationQueryOptions</a> | null</code>\n        </td>\n        <td>\n          <p data-ng_translator_product=\"100\" data-ng_translator_ref_id=\"f52svg2klbbzc2kqsavjdmqpt\">一個查詢選項物件。</p>\n          \n        </td>\n      </tr>\n    </tbody>\n  </table>\n</section>\n  \n</div>\n</article>\n\n<!-- links to this doc:\n - api/animations\n - api/animations/AnimationMetadata\n - api/animations/query\n-->\n<!-- links from this doc:\n - /api\n - api/animations\n - api/animations/AnimationMetadata\n - api/animations/AnimationMetadata#type\n - api/animations/AnimationQueryMetadata#animation\n - api/animations/AnimationQueryMetadata#animationquerymetadata\n - api/animations/AnimationQueryMetadata#instance-properties\n - api/animations/AnimationQueryMetadata#options\n - api/animations/AnimationQueryMetadata#selector\n - api/animations/AnimationQueryOptions\n - api/animations/animation\n - api/animations/query\n - https://github.com/ng-docs/angular-cn/tree/17.3.0-next.1/packages/animations/src/animation_metadata.ts#L282-L302\n-->"}